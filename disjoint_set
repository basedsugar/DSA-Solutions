void makeset()
{
    for (int i = 0; i < n; i++)
    {
        parent[i] = i;
        rank[i] = 0;
    }
}
int findpar(int u)
{
    if (parent[u] == u)
        return u;
    return parent[u] = findpar(parent[u]); //path compression
}
void union(int u, int v)
{
    u = findpar(u);
    v = findpar(v);
    if(rank[u]<rank[v)
        parent[u] = v;
    else if (rank[u] > rank[v])
        parent[v] = u;
    else
    {
        parent[u] = v;
        rank[u]++;
    }
}
